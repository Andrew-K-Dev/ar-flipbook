{
  "version": 2,
  "builds": [
    {
      "src": "ar-view.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.glb",
      "use": "@vercel/static"
    },
    {
      "src": "*.usdz",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
